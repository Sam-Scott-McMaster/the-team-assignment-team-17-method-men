<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object id="window" class="GtkWindow">
    <property name="title">Calc</property>
    <property name="default-width">100</property>
    <child>
      <object id="grid" class="GtkGrid">
        <property name="halign">fill</property>
        <property name="valign">center</property>
        <child>
          <object id="input_label" class="GtkLabel">
            <property name="label"></property>
            <property name="halign">fill</property>
            <property name="hexpand">true</property>
            <property name="valign">center</property>
            <property name="css-classes">calculator-screen</property>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="column-span">5</property>
            </layout>
          </object>
        </child>
        <!-- Row 1 -->
        <child>
          <object id="button7" class="GtkButton">
            <property name="label">7</property>
            <layout>
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button8" class="GtkButton">
            <property name="label">8</property>
            <layout>
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button9" class="GtkButton">
            <property name="label">9</property>
            <layout>
              <property name="column">2</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_negative" class="GtkButton">
            <property name="label">+/-</property>
            <layout>
              <property name="column">3</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_clear" class="GtkButton">
            <property name="label">Clear</property>
            <layout>
              <property name="column">4</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <!-- Row 2 -->
        <child>
          <object id="button4" class="GtkButton">
            <property name="label">4</property>
            <layout>
              <property name="column">0</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button5" class="GtkButton">
            <property name="label">5</property>
            <layout>
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button6" class="GtkButton">
            <property name="label">6</property>
            <layout>
              <property name="column">2</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_add" class="GtkButton">
            <property name="label">+</property>
            <layout>
              <property name="column">3</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_subtract" class="GtkButton">
            <property name="label">-</property>
            <layout>
              <property name="column">4</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <!-- Row 3 -->
        <child>
          <object id="button1" class="GtkButton">
            <property name="label">1</property>
            <layout>
              <property name="column">0</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button2" class="GtkButton">
            <property name="label">2</property>
            <layout>
              <property name="column">1</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button3" class="GtkButton">
            <property name="label">3</property>
            <layout>
              <property name="column">2</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_divide" class="GtkButton">
            <property name="label">รท</property>
            <layout>
              <property name="column">3</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_multiply" class="GtkButton">
            <property name="label">x</property>
            <layout>
              <property name="column">4</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <!-- Row 4 -->
        <child>
          <object id="button0" class="GtkButton">
            <property name="label">0</property>
            <layout>
              <property name="column">0</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_parentheses_open" class="GtkButton">
            <property name="label">(</property>
            <layout>
              <property name="column">1</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_parentheses_close" class="GtkButton">
            <property name="label">)</property>
            <layout>
              <property name="column">2</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object id="button_equal" class="GtkButton">
            <property name="label">=</property>
            <layout>
              <property name="column">3</property>
              <property name="row">4</property>
              <property name="column-span">2</property>
            </layout>
          </object>
        </child>
        <!-- Row 5 -->
        <child>
          <object id="button_help" class="GtkButton">
            <property name="label">Help</property>
            <layout>
              <property name="column">0</property>
              <property name="row">5</property>
              <property name="column-span">5</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>